{
    "title": "HTML Renderer",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "html.schema.json",
    "type": "object",
    "description": "Some HTML to be rendered in the form",
    "properties": {
        "type": {
            "type": "string",
            "const": "HTML"
        },
        "htmlData": {
            "type": "string"
        },
        "showOn": {
            "$ref": "show_on.schema.json"
        }
    },
    "additionalProperties": false,
    "required": ["htmlData"]
}
